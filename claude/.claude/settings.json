      "Bash(go build:*)",
      "Bash(go run:*)",
      "Bash(go mod:*)",
      "Bash(go fmt:*)",
      "Bash(go vet:*)",
      "Bash(gofmt:*)",
      "Bash(goimports:*)",
      "Bash(gci:*)",
      "Bash(golangci-lint:*)",
      "Bash(staticcheck:*)",
      "Bash(deadcode:*)",
      "Bash(ENV=test go test:*)",
      "Bash(make:*)",
      "Bash(docker:*)",
      "Bash(docker compose:*)",
      "Bash(cargo build:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git diff:*)",
      "Bash(git status:*)",
      "Bash(git rebase:*)",
