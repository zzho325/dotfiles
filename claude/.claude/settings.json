      "Bash(sed:*)",
      "Bash(cut:*)",
      "Bash(tr:*)",
      "Bash(tee:*)",
      "Bash(file:*)",
      "Bash(stat:*)",
      "Bash(readlink:*)",
      "Bash(realpath:*)",
      "Bash(basename:*)",
      "Bash(dirname:*)",
      "Bash(pwd:*)",
      "Bash(env:*)",
      "Bash(whoami:*)",
      "Bash(date:*)",
      "Bash(uname:*)",
      "Bash(id:*)",
      "Bash(md5sum:*)",
      "Bash(shasum:*)",
