      "Bash(gh pr list:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr checks:*)",
      "Bash(gh pr diff:*)",
      "Bash(gh issue:*)",
      "Bash(gh api:*)",
      "Bash(lazygit:*)",
      "Bash(orch:*)",
      "Bash(tmux capture-pane:*)",
      "Bash(linear issue view:*)",
      "Bash(columnctl db console:*)",
      "Bash(columnctl db schema:*)",
      "Bash(columnctl db:* -e dev:*)",
      "Bash(columnctl db:* -e staging:*)",
      "Bash(columnctl releases:*)",
      "Bash(columnctl --help:*)",
      "Bash(columnctl * --help:*)",
      "Bash(columnctl tailscale status)",
      "Bash(kubectl config get-contexts:*)",
      "Bash(aws s3 ls:*)",
      "Skill(linear-cli)",
      "mcp__linear-server__get_issue",
      "mcp__claude_ai_Linear__*",
      "mcp__claude_ai_Slack__slack_search_public_and_private",
      "mcp__claude_ai_Slack__slack_search_public",
      "mcp__claude_ai_Slack__slack_read_thread",
      "mcp__claude_ai_Slack__slack_read_channel",
      "mcp__claude_ai_Slack__slack_send_message_draft",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:linear.app)",
      "WebFetch(domain:column-na.slack.com)",
      "WebFetch(domain:app.frbservices.org)",
      "WebFetch(domain:myndex.ai)",
      "WebSearch"
    ],
    "deny": [
      "Bash(gh pr comment:*)",
      "Bash(gh pr review:*)",
      "Bash(gh pr merge:*)",
      "Bash(terraform apply:*)",
      "Bash(aws sts get-caller-identity:*)",
      "Bash(aws configure list:*)",
      "Bash(cat ~/.aws/credentials:*)",
      "Bash(cat ~/.aws/config:*)",
      "Read(/Users/*/.aws/credentials)",
      "Read(/Users/*/.aws/config)",
      "mcp__claude_ai_Slack__slack_send_message",
      "mcp__claude_ai_Slack__slack_schedule_message"
    ]
  },
  "enabledPlugins": {
    "gopls-lsp@claude-plugins-official": true
  },
  "skipDangerousModePermissionPrompt": true
}
